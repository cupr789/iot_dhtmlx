<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="con">
  <select id="selectConnectionInfo" parameterType="con"  resultType="con">
  	select
  	ciNo,ciName,ciUrl,ciPort,ciDatabase,ciUser,ciPwd,ciEtc
  	from connection_info
  </select>

  <insert id="insertConnectionInfo" parameterType="con">
  	insert into connection_info(ciName,ciUrl,ciPort,ciDatabase,ciUser,ciPwd,uiId,ciEtc) 
  	values(#{ciName},#{ciUrl},#{ciPort},#{ciDatabase},#{ciUser},#{ciPwd},#{uiId},#{ciEtc})
  </insert>

  </mapper>